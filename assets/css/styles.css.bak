:root {
    --primary: #357a3a;
    --primary-dark: #2a6230;
    --primary-light: #4a8e4f;
    --secondary: #e3ff00;
    --secondary-dark: #c4d800;
    --secondary-light: #f0ff4d;
    --light: #fff7d9;
    --white: #FFFFFF;
    --black: #000000;
    --gray: #f5f5f5;
    --dark-gray: #333333;
    --text-color: #333333;
    --border-radius: 10px;
    --box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
    --transition: all 0.3s ease;
}

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
}

body {
    background-color: var(--white);
    color: var(--text-color);
    line-height: 1.6;
    overflow-x: hidden;
}

.container {
    width: 100%;
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
}

/* Navbar Moderno */
.navbar {
    background-color: var(--white);
    box-shadow: 0 2px 15px rgba(0, 0, 0, 0.08);
    position: sticky;
    top: 0;
    z-index: 100;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 1rem 5%;
    transition: var(--transition);
}

.logo-container {
    display: flex;
    align-items: center;
}

.logo-link {
    display: flex;
    align-items: center;
    text-decoration: none;
}

.logo-img {
    height: 50px;
    transition: var(--transition);
}

.logo-img:hover {
    transform: scale(1.05);
}

.menu-toggle {
    display: none;
    background: none;
    border: none;
    font-size: 1.5rem;
    cursor: pointer;
    color: var(--primary);
    transition: var(--transition);
}

.menu-toggle:hover {
    color: var(--primary-dark);
}

.nav-links {
    display: flex;
    list-style: none;
    margin: 0;
    padding: 0;
}

.nav-links li {
    position: relative;
    padding: 0 1rem;
}

.nav-links a {
    color: var(--text-color);
    text-decoration: none;
    font-weight: 500;
    transition: var(--transition);
    padding: 0.5rem 0;
    display: block;
    position: relative;
}

.nav-links a:hover, .nav-links .active > a {
    color: var(--primary);
}

.nav-links a::after {
    content: '';
    position: absolute;
    width: 0;
    height: 2px;
    bottom: 0;
    left: 0;
    background-color: var(--primary);
    transition: var(--transition);
}

.nav-links a:hover::after, .nav-links .active > a::after {
    width: 100%;
}

.dropdown {
    position: relative;
}

.dropdown > a {
    display: flex;
    align-items: center;
    gap: 0.3rem;
}

.dropdown-content {
    display: none;
    position: absolute;
    background-color: var(--white);
    min-width: 220px;
    box-shadow: var(--box-shadow);
    z-index: 1;
    border-radius: var(--border-radius);
    overflow: hidden;
    top: 100%;
    left: 0;
    transform: translateY(10px);
    opacity: 0;
    transition: var(--transition);
}

.dropdown:hover .dropdown-content {
    display: block;
    transform: translateY(0);
    opacity: 1;
}

.dropdown-content a {
    padding: 12px 16px;
    border-bottom: 1px solid rgba(0, 0, 0, 0.05);
    transition: var(--transition);
}

.dropdown-content a:hover {
    background-color: var(--light);
    padding-left: 20px;
}

/* Hero Section con Video Mejorado */
.hero {
    position: relative;
    height: 85vh;
    min-height: 600px;
    overflow: hidden;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    color: var(--white);
}

.video-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.video-background video {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.video-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.6));
    z-index: 1;
}

.hero-content {
    position: relative;
    z-index: 2;
    max-width: 800px;
    padding: 0 2rem;
    animation: fadeInUp 1.5s ease;
}

@keyframes fadeInUp {
    from { opacity: 0; transform: translateY(30px); }
    to { opacity: 1; transform: translateY(0); }
}

.hero h1 {
    font-size: 4rem;
    margin-bottom: 1rem;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);
    font-weight: 800;
    letter-spacing: -1px;
}

.hero p {
    font-size: 1.6rem;
    margin-bottom: 2rem;
    text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);
    font-weight: 300;
}

.cta-button {
    display: inline-block;
    background-color: var(--primary);
    color: var(--white);
    padding: 1rem 2.5rem;
    border-radius: 50px;
    text-decoration: none;
    font-weight: bold;
    transition: var(--transition);
    border: 2px solid var(--primary);
    font-size: 1.1rem;
    box-shadow: 0 4px 15px rgba(53, 122, 58, 0.3);
    position: relative;
    overflow: hidden;
    z-index: 1;
}

.cta-button::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 0%;
    height: 100%;
    background-color: var(--secondary);
    transition: var(--transition);
    z-index: -1;
}

.cta-button:hover {
    color: var(--primary-dark);
    border-color: var(--secondary);
    transform: translateY(-3px);
    box-shadow: 0 10px 20px rgba(53, 122, 58, 0.4);
}

.cta-button:hover::before {
    width: 100%;
}

/* Servicios Educativos Modernos */
.services-section {
    padding: 6rem 10%;
    background-color: var(--white);
}

.platforms-title {
    text-align: center;
    margin-bottom: 3.5rem;
    color: var(--primary);
    font-size: 2.2rem;
    position: relative;
    padding-bottom: 15px;
    font-weight: 700;
}

.platforms-title::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 80px;
    height: 4px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    border-radius: 2px;
}

.services-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
    gap: 2.5rem;
}

.service-card {
    background-color: var(--white);
    border-radius: var(--border-radius);
    padding: 2.5rem;
    transition: var(--transition);
    border-left: 4px solid var(--primary);
    box-shadow: var(--box-shadow);
    position: relative;
    overflow: hidden;
    z-index: 1;
}

.service-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, rgba(53, 122, 58, 0.05), rgba(227, 255, 0, 0.05));
    z-index: -1;
    transition: var(--transition);
    opacity: 0;
}

.service-card:hover {
    transform: translateY(-15px);
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
    border-left: 4px solid var(--secondary);
}

.service-card:hover::before {
    opacity: 1;
}

.service-icon {
    font-size: 3rem;
    color: var(--primary);
    margin-bottom: 1.5rem;
    transition: var(--transition);
}

.service-card:hover .service-icon {
    color: var(--secondary-dark);
    transform: scale(1.1);
}

.service-content h4 {
    color: var(--primary-dark);
    margin-bottom: 1rem;
    font-size: 1.4rem;
    font-weight: 700;
    transition: var(--transition);
}

.service-card:hover .service-content h4 {
    color: var(--primary);
}

.service-content p {
    color: var(--text-color);
    margin-bottom: 1.5rem;
    line-height: 1.7;
    font-size: 1.05rem;
}

.service-link {
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    color: var(--primary);
    text-decoration: none;
    font-weight: 600;
    transition: var(--transition);
    padding: 0.5rem 0;
    position: relative;
}

.service-link::after {
    content: '';
    position: absolute;
    width: 0;
    height: 2px;
    bottom: 0;
    left: 0;
    background-color: var(--primary);
    transition: var(--transition);
}

.service-link:hover {
    color: var(--primary-dark);
}

.service-link:hover::after {
    width: 100%;
}

.service-link i {
    font-size: 0.8rem;
    transition: var(--transition);
}

.service-link:hover i {
    transform: translateX(5px);
}

/* Noticias Modernas */
.news-section {
    background-color: var(--gray);
    padding: 6rem 10%;
    position: relative;
    overflow: hidden;
}

.news-section::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, rgba(53, 122, 58, 0.03), rgba(227, 255, 0, 0.03));
    z-index: 0;
}

.news-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
    gap: 2.5rem;
    margin: 3rem 0;
    position: relative;
    z-index: 1;
}

.news-card {
    background-color: var(--white);
    border-radius: var(--border-radius);
    overflow: hidden;
    box-shadow: var(--box-shadow);
    transition: var(--transition);
    position: relative;
}

.news-card:hover {
    transform: translateY(-15px);
    box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
}

.news-image {
    position: relative;
    height: 220px;
    overflow: hidden;
}

.news-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: var(--transition);
}

.news-card:hover .news-image img {
    transform: scale(1.1);
}

.news-date {
    position: absolute;
    top: 20px;
    right: 20px;
    background: linear-gradient(135deg, var(--primary), var(--primary-dark));
    color: var(--white);
    padding: 0.7rem 1.2rem;
    border-radius: var(--border-radius);
    text-align: center;
    line-height: 1.2;
    box-shadow: 0 5px 15px rgba(53, 122, 58, 0.3);
}

.news-date .day {
    font-size: 1.8rem;
    font-weight: bold;
    display: block;
}

.news-date .month {
    font-size: 1rem;
    text-transform: uppercase;
    font-weight: 500;
}

.news-content {
    padding: 2rem;
}

.news-content h3 {
    color: var(--primary-dark);
    margin-bottom: 1rem;
    font-size: 1.4rem;
    font-weight: 700;
    transition: var(--transition);
}

.news-card:hover .news-content h3 {
    color: var(--primary);
}

.news-content p {
    margin-bottom: 1.5rem;
    color: var(--text-color);
    line-height: 1.7;
}

.read-more {
    color: var(--primary);
    text-decoration: none;
    font-weight: 600;
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    transition: var(--transition);
    position: relative;
    padding-bottom: 5px;
}

.read-more::after {
    content: '';
    position: absolute;
    width: 0;
    height: 2px;
    bottom: 0;
    left: 0;
    background-color: var(--primary);
    transition: var(--transition);
}

.read-more:hover {
    color: var(--primary-dark);
}

.read-more:hover::after {
    width: 100%;
}

.read-more i {
    font-size: 0.8rem;
    transition: var(--transition);
}

.read-more:hover i {
    transform: translateX(5px);
}

.news-cta {
    text-align: center;
    margin-top: 3rem;
    position: relative;
    z-index: 1;
}

/* Sección de Redes Sociales Moderna */
.social-media-section {
    padding: 6rem 10%;
    background-color: var(--white);
    position: relative;
    overflow: hidden;
}

.social-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 2rem;
    margin-top: 3rem;
}

.social-card {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 3rem 2rem;
    border-radius: var(--border-radius);
    color: var(--white);
    text-decoration: none;
    text-align: center;
    transition: var(--transition);
    position: relative;
    overflow: hidden;
    z-index: 1;
    box-shadow: var(--box-shadow);
}

.social-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: inherit;
    z-index: -1;
    transition: var(--transition);
}

.social-card:hover::before {
    transform: scale(1.1);
}

.social-card i {
    font-size: 3rem;
    margin-bottom: 1.5rem;
    transition: var(--transition);
}

.social-card:hover i {
    transform: scale(1.2);
}

.social-card h3 {
    font-size: 1.5rem;
    margin-bottom: 0.5rem;
    font-weight: 700;
}

.social-card p {
    font-size: 1.1rem;
    opacity: 0.9;
}

.facebook {
    background: linear-gradient(135deg, #3b5998, #1e3c72);
}

.instagram {
    background: linear-gradient(45deg, #f09433, #e6683c, #dc2743, #cc2366, #bc1888);
}

.youtube {
    background: linear-gradient(135deg, #FF0000, #CC0000);
}

/* WhatsApp Button Mejorado */
.whatsapp-button {
    position: fixed;
    bottom: 30px;
    right: 30px;
    background: linear-gradient(135deg, #25D366, #128C7E);
    color: white;
    width: 60px;
    height: 60px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.8rem;
    box-shadow: 0 5px 15px rgba(37, 211, 102, 0.4);
    z-index: 99;
    transition: var(--transition);
    animation: pulse 2s infinite;
    opacity: 0;
    visibility: hidden;
}

.whatsapp-button.visible {
    opacity: 1;
    visibility: visible;
}

.whatsapp-button:hover {
    transform: scale(1.1) rotate(10deg);
    animation: none;
}

@keyframes pulse {
    0% {
        box-shadow: 0 0 0 0 rgba(37, 211, 102, 0.7);
    }
    70% {
        box-shadow: 0 0 0 15px rgba(37, 211, 102, 0);
    }
    100% {
        box-shadow: 0 0 0 0 rgba(37, 211, 102, 0);
    }
}

/* Footer Moderno */
footer {
    background: linear-gradient(135deg, var(--primary-dark), var(--primary));
    color: var(--white);
    padding: 5rem 10% 3rem;
    position: relative;
    overflow: hidden;
}

footer::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at top right, rgba(227, 255, 0, 0.1), transparent 70%);
    z-index: 0;
}

.footer-content {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 3rem;
    position: relative;
    z-index: 1;
}

.footer-column h3 {
    margin-bottom: 1.5rem;
    font-size: 1.4rem;
    position: relative;
    padding-bottom: 12px;
    font-weight: 700;
}

.footer-column h3::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 50px;
    height: 3px;
    background-color: var(--secondary);
    border-radius: 2px;
}

.footer-column p, .footer-column a {
    margin-bottom: 1rem;
    display: block;
    color: var(--white);
    text-decoration: none;
    transition: var(--transition);
    opacity: 0.9;
    font-size: 1.05rem;
}

.footer-column a:hover {
    color: var(--secondary);
    opacity: 1;
    transform: translateX(5px);
}

.footer-column i {
    margin-right: 10px;
    color: var(--secondary);
}

.map-link {
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    margin-top: 1rem;
    font-weight: 600;
    padding: 0.5rem 1rem;
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 30px;
    transition: var(--transition);
}

.map-link:hover {
    background-color: var(--secondary);
    color: var(--primary-dark);
    transform: translateX(0) translateY(-3px);
}

.social-links {
    display: flex;
    gap: 1rem;
    margin: 1.5rem 0;
}

.social-links a {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    width: 40px;
    height: 40px;
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 50%;
    transition: var(--transition);
    color: var(--white);
}

.social-links a:hover {
    background-color: var(--secondary);
    color: var(--primary-dark);
    transform: translateY(-3px);
}

.newsletter h4 {
    margin-bottom: 1rem;
    font-size: 1.2rem;
}

.newsletter input {
    width: 100%;
    padding: 1rem;
    border: none;
    border-radius: 30px;
    margin-bottom: 1rem;
    background-color: rgba(255, 255, 255, 0.1);
    color: var(--white);
    transition: var(--transition);
}

.newsletter input:focus {
    outline: none;
    background-color: rgba(255, 255, 255, 0.2);
}

.newsletter input::placeholder {
    color: rgba(255, 255, 255, 0.7);
}

.newsletter button {
    background-color: var(--secondary);
    color: var(--primary-dark);
    border: none;
    padding: 0.8rem 1.5rem;
    border-radius: 30px;
    cursor: pointer;
    font-weight: bold;
    transition: var(--transition);
    box-shadow: 0 4px 10px rgba(227, 255, 0, 0.3);
}

.newsletter button:hover {
    background-color: var(--secondary-light);
    transform: translateY(-3px);
    box-shadow: 0 6px 15px rgba(227, 255, 0, 0.4);
}

.copyright {
    text-align: center;
    margin-top: 4rem;
    padding-top: 2rem;
    border-top: 1px solid rgba(255, 255, 255, 0.1);
    position: relative;
    z-index: 1;
    font-size: 1.05rem;
    opacity: 0.8;
}

/* Página Interna - Encabezado */
.page-header {
    background: linear-gradient(135deg, var(--primary), var(--primary-dark));
    color: var(--white);
    padding: 4rem 0;
    text-align: center;
    position: relative;
    overflow: hidden;
}

.page-header::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at center, rgba(227, 255, 0, 0.1), transparent 70%);
    z-index: 0;
}

.page-header h1 {
    font-size: 3rem;
    margin-bottom: 1rem;
    position: relative;
    z-index: 1;
    font-weight: 800;
}

.breadcrumb {
    display: flex;
    justify-content: center;
    list-style: none;
    padding: 0;
    margin: 0;
    position: relative;
    z-index: 1;
}

.breadcrumb li {
    display: inline;
    font-size: 1.1rem;
}

.breadcrumb li:not(:last-child)::after {
    content: '/';
    margin: 0 10px;
    color: rgba(255, 255, 255, 0.7);
}

.breadcrumb a {
    color: var(--white);
    text-decoration: none;
    transition: var(--transition);
}

.breadcrumb a:hover {
    color: var(--secondary);
}

.breadcrumb .active {
    color: var(--secondary);
    font-weight: 600;
}

/* Contenido de Página */
.page-content {
    min-height: 60vh;
}

/* Sección de Historia */
.history-section {
    padding: 5rem 0;
}

.history-content {
    display: grid;
    grid-template-columns: 1fr 2fr;
    gap: 3rem;
    align-items: center;
}

.history-image {
    text-align: center;
}

.history-image img {
    max-width: 100%;
    border-radius: var(--border-radius);
    box-shadow: var(--box-shadow);
    transition: var(--transition);
}

.history-image img:hover {
    transform: scale(1.03);
}

.history-text h2 {
    color: var(--primary);
    margin-bottom: 1.5rem;
    font-size: 2rem;
    position: relative;
    padding-bottom: 10px;
}

.history-text h2::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 60px;
    height: 3px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    border-radius: 2px;
}

.history-text p {
    margin-bottom: 2rem;
    line-height: 1.8;
    font-size: 1.1rem;
    color: var(--text-color);
}

/* Sección de Contacto */
.contact-section {
    padding: 5rem 0;
}

.contact-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 3rem;
}

.contact-info h2 {
    color: var(--primary);
    margin-bottom: 1.5rem;
    font-size: 2rem;
    position: relative;
    padding-bottom: 10px;
}

.contact-info h2::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 60px;
    height: 3px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    border-radius: 2px;
}

.contact-info p {
    margin-bottom: 2rem;
    line-height: 1.8;
    font-size: 1.1rem;
}

.info-item {
    display: flex;
    margin-bottom: 2rem;
}

.info-icon {
    flex: 0 0 60px;
    height: 60px;
    background-color: var(--primary);
    color: var(--white);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.5rem;
    margin-right: 1.5rem;
    transition: var(--transition);
}

.info-item:hover .info-icon {
    background-color: var(--secondary);
    color: var(--primary-dark);
    transform: rotateY(180deg);
}

.info-content h3 {
    color: var(--primary-dark);
    margin-bottom: 0.5rem;
    font-size: 1.3rem;
}

.info-content p {
    margin-bottom: 0.5rem;
    line-height: 1.6;
}

.contact-form {
    background-color: var(--white);
    padding: 2.5rem;
    border-radius: var(--border-radius);
    box-shadow: var(--box-shadow);
}

.contact-form h2 {
    color: var(--primary);
    margin-bottom: 1.5rem;
    font-size: 2rem;
    position: relative;
    padding-bottom: 10px;
}

.contact-form h2::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 60px;
    height: 3px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    border-radius: 2px;
}

.form-group {
    margin-bottom: 1.5rem;
}

.form-group label {
    display: block;
    margin-bottom: 0.5rem;
    color: var(--text-color);
    font-weight: 500;
}

.form-control {
    width: 100%;
    padding: 1rem;
    border: 1px solid #ddd;
    border-radius: var(--border-radius);
    transition: var(--transition);
}

.form-control:focus {
    outline: none;
    border-color: var(--primary);
    box-shadow: 0 0 0 3px rgba(53, 122, 58, 0.1);
}

textarea.form-control {
    min-height: 150px;
    resize: vertical;
}

.submit-btn {
    background: linear-gradient(135deg, var(--primary), var(--primary-dark));
    color: var(--white);
    border: none;
    padding: 1rem 2rem;
    border-radius: 30px;
    font-weight: bold;
    cursor: pointer;
    transition: var(--transition);
    box-shadow: 0 4px 10px rgba(53, 122, 58, 0.3);
    width: 100%;
}

.submit-btn:hover {
    background: linear-gradient(135deg, var(--primary-dark), var(--primary));
    transform: translateY(-3px);
    box-shadow: 0 6px 15px rgba(53, 122, 58, 0.4);
}

/* Sección de Admisiones */
.admissions-section {
    padding: 5rem 0;
}

.section-intro {
    text-align: center;
    max-width: 800px;
    margin: 0 auto 4rem;
}

.section-intro h2 {
    color: var(--primary);
    margin-bottom: 1.5rem;
    font-size: 2.2rem;
    position: relative;
    padding-bottom: 15px;
    display: inline-block;
}

.section-intro h2::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 80px;
    height: 3px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    border-radius: 2px;
}

.section-intro p {
    font-size: 1.1rem;
    line-height: 1.8;
    color: var(--text-color);
}

.admissions-steps h3 {
    text-align: center;
    color: var(--primary-dark);
    margin-bottom: 3rem;
    font-size: 1.8rem;
}

.step-cards {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 2rem;
}

.step-card {
    background-color: var(--white);
    border-radius: var(--border-radius);
    padding: 2.5rem;
    box-shadow: var(--box-shadow);
    transition: var(--transition);
    position: relative;
    overflow: hidden;
    z-index: 1;
}

.step-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 5px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    transition: var(--transition);
}

.step-card:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
}

.step-card:hover::before {
    height: 10px;
}

.step-number {
    width: 50px;
    height: 50px;
    background-color: var(--primary);
    color: var(--white);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.5rem;
    font-weight: bold;
    margin-bottom: 1.5rem;
    transition: var(--transition);
}

.step-card:hover .step-number {
    background-color: var(--secondary);
    color: var(--primary-dark);
    transform: scale(1.1);
}

.step-content h4 {
    color: var(--primary-dark);
    margin-bottom: 1rem;
    font-size: 1.4rem;
}

.step-content p {
    margin-bottom: 1.5rem;
    line-height: 1.7;
    color: var(--text-color);
}

.step-link {
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    color: var(--primary);
    text-decoration: none;
    font-weight: 600;
    transition: var(--transition);
    position: relative;
    padding-bottom: 5px;
}

.step-link::after {
    content: '';
    position: absolute;
    width: 0;
    height: 2px;
    bottom: 0;
    left: 0;
    background-color: var(--primary);
    transition: var(--transition);
}

.step-link:hover {
    color: var(--primary-dark);
}

.step-link:hover::after {
    width: 100%;
}

/* Responsive Design Mejorado */
@media (max-width: 1200px) {
    .hero h1 {
        font-size: 3.5rem;
    }
    
    .hero p {
        font-size: 1.4rem;
    }
    
    .services-section, .news-section, .social-media-section {
        padding: 5rem 5%;
    }
    
    .history-content {
        grid-template-columns: 1fr;
        text-align: center;
    }
    
    .history-text h2::after {
        left: 50%;
        transform: translateX(-50%);
    }
    
    .history-image {
        margin-bottom: 2rem;
    }
}

@media (max-width: 992px) {
    .hero h1 {
        font-size: 3rem;
    }
    
    .hero p {
        font-size: 1.3rem;
    }
    
    .services-section, .news-section, .social-media-section {
        padding: 4rem 5%;
    }
    
    .platforms-title {
        font-size: 2rem;
    }
    
    .contact-grid {
        grid-template-columns: 1fr;
    }
    
    .contact-info {
        margin-bottom: 2rem;
    }
}

@media (max-width: 768px) {
    .menu-toggle {
        display: block;
    }
    
    .nav-links {
        display: none;
        flex-direction: column;
        width: 100%;
        position: absolute;
        top: 100%;
        left: 0;
        background-color: var(--white);
        box-shadow: 0 10px 15px rgba(0, 0, 0, 0.1);
        padding: 1rem;
        z-index: 100;
    }
    
    .nav-links.active {
        display: flex;
    }
    
    .nav-links li {
        padding: 0.8rem 0;
        border-bottom: 1px solid rgba(0, 0, 0, 0.05);
    }
    
    .dropdown-content {
        position: static;
        box-shadow: none;
        display: none;
        margin-left: 1rem;
        transform: none;
        opacity: 1;
    }
    
    .dropdown:hover .dropdown-content {
        display: none;
    }
    
    .dropdown.active .dropdown-content {
        display: block;
    }
    
    .hero {
        height: 70vh;
    }
    
    .hero h1 {
        font-size: 2.5rem;
    }
    
    .hero p {
        font-size: 1.2rem;
    }
    
    .services-section, .news-section, .social-media-section {
        padding: 3rem 5%;
    }
    
    .platforms-title {
        font-size: 1.8rem;
        margin-bottom: 2.5rem;
    }
    
    .footer-content {
        grid-template-columns: 1fr;
        gap: 2rem;
    }
    
    .page-header h1 {
        font-size: 2.5rem;
    }
}

@media (max-width: 576px) {
    .hero h1 {
        font-size: 2rem;
    }
    
    .hero p {
        font-size: 1.1rem;
    }
    
    .cta-button {
        padding: 0.8rem 1.8rem;
        font-size: 1rem;
    }
    
    .services-grid, .news-grid, .social-grid, .step-cards {
        grid-template-columns: 1fr;
    }
    
    .page-header {
        padding: 3rem 0;
    }
    
    .page-header h1 {
        font-size: 2rem;
    }
    
    .breadcrumb li {
        font-size: 0.9rem;
    }
    
    .section-intro h2, .contact-info h2, .contact-form h2, .history-text h2 {
        font-size: 1.8rem;
    }
    
    .whatsapp-button {
        width: 50px;
        height: 50px;
        font-size: 1.5rem;
        bottom: 20px;
        right: 20px;
    }
}

/* Sección de Estadísticas Moderna */
.stats-section {
    padding: 3rem 0;  /* Reducido de 5rem a 3rem */
    margin-top: 1cm;  /* Añadido margen superior de 1 cm */
    background: linear-gradient(135deg, rgba(53, 122, 58, 0.95), rgba(42, 98, 48, 0.95)), url('../images/pattern-bg.png');
    background-size: cover;
    background-attachment: fixed;
    color: var(--white);
    position: relative;
    overflow: hidden;
}

.stats-section::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at center, rgba(227, 255, 0, 0.2), transparent 70%);
    z-index: 0;
}

.stats-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 2.5rem;
    position: relative;
    z-index: 1;
}

.stat-card {
    background: rgba(255, 255, 255, 0.1);
    backdrop-filter: blur(10px);
    border-radius: 20px;
    padding: 2.5rem 2rem;
    text-align: center;
    transition: transform 0.4s ease, box-shadow 0.4s ease;
    border: 1px solid rgba(255, 255, 255, 0.2);
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.15);
    position: relative;
    overflow: hidden;
}

.stat-card::before {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: radial-gradient(circle, rgba(227, 255, 0, 0.1), transparent 70%);
    opacity: 0;
    transform: scale(0.5);
    transition: transform 0.5s ease, opacity 0.5s ease;
    z-index: -1;
}

.stat-card:hover {
    transform: translateY(-15px);
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.2);
}

.stat-card:hover::before {
    opacity: 1;
    transform: scale(1);
}

.stat-icon {
    font-size: 3rem;
    margin-bottom: 1.5rem;
    color: var(--secondary);
    transition: transform 0.5s ease;
}

.stat-card:hover .stat-icon {
    transform: scale(1.2) rotate(10deg);
}

.stat-content h3 {
    font-size: 3.5rem;
    font-weight: 800;
    margin-bottom: 0.5rem;
    background: linear-gradient(to right, var(--white), var(--secondary));
    -webkit-background-clip: text;
    background-clip: text;
    -webkit-text-fill-color: transparent;
    display: inline-block;
}

.stat-content p {
    font-size: 1.2rem;
    font-weight: 500;
    color: var(--light);
    position: relative;
    display: inline-block;
}

.stat-content p::after {
    content: '';
    position: absolute;
    bottom: -8px;
    left: 50%;
    transform: translateX(-50%);
    width: 40px;
    height: 3px;
    background: var(--secondary);
    border-radius: 10px;
    transition: width 0.3s ease;
}

.stat-card:hover .stat-content p::after {
    width: 60px;
}

@media (max-width: 992px) {
    .stats-grid {
        grid-template-columns: repeat(2, 1fr);
    }
}

@media (max-width: 576px) {
    .stats-grid {
        grid-template-columns: 1fr;
    }
    
    .stat-card {
        padding: 2rem 1.5rem;
    }
    
    .stat-content h3 {
        font-size: 3rem;
    }
}

.news-cta {
    text-align: center;
    margin-top: 3rem;
    position: relative;
    z-index: 1;
}

/* Sección de Redes Sociales Moderna */
.social-media-section {
    padding: 6rem 10%;
    background-color: var(--white);
    position: relative;
    overflow: hidden;
}

.social-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 2rem;
    margin-top: 3rem;
}

.social-card {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 3rem 2rem;
    border-radius: var(--border-radius);
    color: var(--white);
    text-decoration: none;
    text-align: center;
    transition: var(--transition);
    position: relative;
    overflow: hidden;
    z-index: 1;
    box-shadow: var(--box-shadow);
}

.social-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: inherit;
    z-index: -1;
    transition: var(--transition);
}

.social-card:hover::before {
    transform: scale(1.1);
}

.social-card i {
    font-size: 3rem;
    margin-bottom: 1.5rem;
    transition: var(--transition);
}

.social-card:hover i {
    transform: scale(1.2);
}

.social-card h3 {
    font-size: 1.5rem;
    margin-bottom: 0.5rem;
    font-weight: 700;
}

.social-card p {
    font-size: 1.1rem;
    opacity: 0.9;
}

.facebook {
    background: linear-gradient(135deg, #3b5998, #1e3c72);
}

.instagram {
    background: linear-gradient(45deg, #f09433, #e6683c, #dc2743, #cc2366, #bc1888);
}

.youtube {
    background: linear-gradient(135deg, #FF0000, #CC0000);
}

/* WhatsApp Button Mejorado */
.whatsapp-button {
    position: fixed;
    bottom: 30px;
    right: 30px;
    background: linear-gradient(135deg, #25D366, #128C7E);
    color: white;
    width: 60px;
    height: 60px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.8rem;
    box-shadow: 0 5px 15px rgba(37, 211, 102, 0.4);
    z-index: 99;
    transition: var(--transition);
    animation: pulse 2s infinite;
    opacity: 0;
    visibility: hidden;
}

.whatsapp-button.visible {
    opacity: 1;
    visibility: visible;
}

.whatsapp-button:hover {
    transform: scale(1.1) rotate(10deg);
    animation: none;
}

@keyframes pulse {
    0% {
        box-shadow: 0 0 0 0 rgba(37, 211, 102, 0.7);
    }
    70% {
        box-shadow: 0 0 0 15px rgba(37, 211, 102, 0);
    }
    100% {
        box-shadow: 0 0 0 0 rgba(37, 211, 102, 0);
    }
}

/* Footer Moderno */
footer {
    background: linear-gradient(135deg, var(--primary-dark), var(--primary));
    color: var(--white);
    padding: 5rem 10% 3rem;
    position: relative;
    overflow: hidden;
}

footer::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at top right, rgba(227, 255, 0, 0.1), transparent 70%);
    z-index: 0;
}

.footer-content {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 3rem;
    position: relative;
    z-index: 1;
}

.footer-column h3 {
    margin-bottom: 1.5rem;
    font-size: 1.4rem;
    position: relative;
    padding-bottom: 12px;
    font-weight: 700;
}

.footer-column h3::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 50px;
    height: 3px;
    background-color: var(--secondary);
    border-radius: 2px;
}

.footer-column p, .footer-column a {
    margin-bottom: 1rem;
    display: block;
    color: var(--white);
    text-decoration: none;
    transition: var(--transition);
    opacity: 0.9;
    font-size: 1.05rem;
}

.footer-column a:hover {
    color: var(--secondary);
    opacity: 1;
    transform: translateX(5px);
}

.footer-column i {
    margin-right: 10px;
    color: var(--secondary);
}

.map-link {
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    margin-top: 1rem;
    font-weight: 600;
    padding: 0.5rem 1rem;
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 30px;
    transition: var(--transition);
}

.map-link:hover {
    background-color: var(--secondary);
    color: var(--primary-dark);
    transform: translateX(0) translateY(-3px);
}

.social-links {
    display: flex;
    gap: 1rem;
    margin: 1.5rem 0;
}

.social-links a {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    width: 40px;
    height: 40px;
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 50%;
    transition: var(--transition);
    color: var(--white);
}

.social-links a:hover {
    background-color: var(--secondary);
    color: var(--primary-dark);
    transform: translateY(-3px);
}

.newsletter h4 {
    margin-bottom: 1rem;
    font-size: 1.2rem;
}

.newsletter input {
    width: 100%;
    padding: 1rem;
    border: none;
    border-radius: 30px;
    margin-bottom: 1rem;
    background-color: rgba(255, 255, 255, 0.1);
    color: var(--white);
    transition: var(--transition);
}

.newsletter input:focus {
    outline: none;
    background-color: rgba(255, 255, 255, 0.2);
}

.newsletter input::placeholder {
    color: rgba(255, 255, 255, 0.7);
}

.newsletter button {
    background-color: var(--secondary);
    color: var(--primary-dark);
    border: none;
    padding: 0.8rem 1.5rem;
    border-radius: 30px;
    cursor: pointer;
    font-weight: bold;
    transition: var(--transition);
    box-shadow: 0 4px 10px rgba(227, 255, 0, 0.3);
}

.newsletter button:hover {
    background-color: var(--secondary-light);
    transform: translateY(-3px);
    box-shadow: 0 6px 15px rgba(227, 255, 0, 0.4);
}

.copyright {
    text-align: center;
    margin-top: 4rem;
    padding-top: 2rem;
    border-top: 1px solid rgba(255, 255, 255, 0.1);
    position: relative;
    z-index: 1;
    font-size: 1.05rem;
    opacity: 0.8;
}

/* Página Interna - Encabezado */
.page-header {
    background: linear-gradient(135deg, var(--primary), var(--primary-dark));
    color: var(--white);
    padding: 4rem 0;
    text-align: center;
    position: relative;
    overflow: hidden;
}

.page-header::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at center, rgba(227, 255, 0, 0.1), transparent 70%);
    z-index: 0;
}

.page-header h1 {
    font-size: 3rem;
    margin-bottom: 1rem;
    position: relative;
    z-index: 1;
    font-weight: 800;
}

.breadcrumb {
    display: flex;
    justify-content: center;
    list-style: none;
    padding: 0;
    margin: 0;
    position: relative;
    z-index: 1;
}

.breadcrumb li {
    display: inline;
    font-size: 1.1rem;
}

.breadcrumb li:not(:last-child)::after {
    content: '/';
    margin: 0 10px;
    color: rgba(255, 255, 255, 0.7);
}

.breadcrumb a {
    color: var(--white);
    text-decoration: none;
    transition: var(--transition);
}

.breadcrumb a:hover {
    color: var(--secondary);
}

.breadcrumb .active {
    color: var(--secondary);
    font-weight: 600;
}

/* Contenido de Página */
.page-content {
    min-height: 60vh;
}

/* Sección de Historia */
.history-section {
    padding: 5rem 0;
}

.history-content {
    display: grid;
    grid-template-columns: 1fr 2fr;
    gap: 3rem;
    align-items: center;
}

.history-image {
    text-align: center;
}

.history-image img {
    max-width: 100%;
    border-radius: var(--border-radius);
    box-shadow: var(--box-shadow);
    transition: var(--transition);
}

.history-image img:hover {
    transform: scale(1.03);
}

.history-text h2 {
    color: var(--primary);
    margin-bottom: 1.5rem;
    font-size: 2rem;
    position: relative;
    padding-bottom: 10px;
}

.history-text h2::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 60px;
    height: 3px;
    background: linear-gradient(to right, var(--primary), var(--secondary));
    border-radius: 2px;
}

.history-text p {
    margin-bottom: 2rem;
    line-height: 1.8;
    font-size: 1.1rem;
    color: var(--text-color);
}

